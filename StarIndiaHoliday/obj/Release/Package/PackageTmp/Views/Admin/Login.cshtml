@model StarIndiaHoliday.Models.Login
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <title>Star India Holiday</title>
    <link rel="stylesheet" href="@Url.Content("~/Content/Admin/css/style.css")">
    <!--[if lt IE 9]>
    <script type="text/javascript" src="html5.js"></script>
    <![endif]-->
    <!--[if lt IE 7.]>
    <script defer type="text/javascript" src="pngfix1.js"></script>
    <![endif]-->
    <!-- Menu start --------------->
    <link href="@Url.Content("~/Content/Admin/menu/quickmenu0.css")" rel="stylesheet" type="text/css" media="screen" />
    <script type="text/javascript" src="@Url.Content("~/Content/Admin/menu/quickmenu0.js")"></script>
    <!-- Menu End --------------->
    @*For Validation start*@
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"></script>
    @*For Validation end*@
   
</head>
<body>
    <header style="border-bottom:2px solid #efefef;">
        <div id="wrap">
            <div class="logo"><img src="@Url.Content("~/Content/Admin/images/logo.png")" border="0"></div>

            <div class="admintxt">Admin panel</div>
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
    </header>

    <div class="clear"></div>
    <div class="bodycont">
        <div id="wrap2" style="min-height:530px;">
            @using (Html.BeginForm())
        {
            @Html.ValidationSummary(true)
            <div class="login-cont">
                <h1 class="loginhd">Login Here</h1>
                <span style="color:red;margin-left:120px;"> @ViewBag.error</span>
                <div class="logininr">
                    <div class="login-row">
                        <div class="loginname">Email</div>
                        <div class="admintxtfld-box">
                            @*<input type="text">*@
                            @Html.TextBoxFor(model => model.UserName)
                        </div>
                        <div class="clear"></div>
                    </div>
                    <!--       <div class="loginreq-field">* This Field Required </div> -->

                    <div class="login-row">
                        <div class="loginname">Password</div>
                        <div class="admintxtfld-box">
                            @*<input type="password">*@
                            @Html.PasswordFor(model => model.Password)
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>

                <div class="clear"></div>
                <div class="contact-row" style="width:94%;">
                    <div style="background:none; border:none; margin-top:15px;">
                        
                            <input type="submit" class="btn" value="Login" style="margin-left:32px;">
                        <br>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
        }
            <div class="clear"></div>
        </div>
        <div class="clear"></div>
    </div>
    <div class="clear"></div>
    <footer>
        <footer class="whitefoter">
            <div class="whitefooter-cont">
                <div style="float:left;">
                    Copyright &copy; 2019 Star India Holiday
                    All Rights Reserved.
                </div>
                <a target="_blank" href="http://www.aks-india.com/" style="float:right;"> <img src="@Url.Content("~/Content/Admin/images/akslogo.jpg")" alt="AKS Websoft Consulting Pvt. Ltd." title="AKS Websoft Consulting Pvt. Ltd."></a>
                <div class="clear"></div>
            </div>
        </footer>
    </footer>
</body>
</html>

